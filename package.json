            },
            {
                "command": "FILE.expand",
                "title": "Refresh this File List",
                "icon": "$(expand-all)",
                "category": "FILE"
            },
            {
                "command": "FILE.collapse",
                "title": "Refresh this File List",
                "icon": "$(collapse-all)",
                "category": "FILE"
            },
            {
                "command": "FILE.openFile",
                "title": "Open this File",
                "category": "FILE"
            },
            {
